{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": ["warn", { "allow": ["log", "warn", "error"]}],
    "react/forbid-prop-types": ["disabled"],
    "no-underscore-dangle": 0 
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  }
}
